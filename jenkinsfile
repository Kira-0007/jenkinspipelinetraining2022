pipeline {

agent any

stages {

	stage('SCM') {
		steps {
			echo "git pull my code for java app"
			git 'https://github.com/Kira-0007/simple-maven-app.git'
		}
	}

	stage('Build') {
		steps {
			sh 'mvn clean package'
		}
	}

	stage('Test') {
		steps {
			echo "test my code"
		}
	}
	stage('Deploy to Prod') {
		steps {
			echo "Deploy to Production"
		}
	}

}



}